    dplyr(>= 0.8.0),
    ggplot2(>= 3.1.0),
    glue(>= 1.3.1),
    reshape2(>= 1.4.3),
    RSQLite(>= 2.1.1),
    stringr(>= 1.4.0)
Encoding: UTF-8
LazyData: TRUE
